  },
  "keywords": [
    "cli",
    "documentation",
    "openapi",
    "byfive"
  ],
  "author": "ByeFive",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^11.0.0",
    "inquirer": "^9.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "typescript": "^5.0.0"
  }
}
