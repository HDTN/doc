{
  "name": "byfive-cli",
  "version": "1.0.0",
  "description": "CLI para gerenciar documentações de API Reference e OpenAPI",
  "main": "dist/cli.js",
  "bin": {
    "byfive-cli": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc && pnpm copy-templates",
    "copy-templates": "cp -r src/templates dist/",
